<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest
	Version="4.0"
	ExtensionBundleId="com.lazy.sorter"
	ExtensionBundleVersion="1.0.0"
	ExtensionBundleName=" Lazy Sorter"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<ExtensionList>
		<Extension Id="com.lazy.sorter.panel" Version="1.0" />
	</ExtensionList>

	<ExecutionEnvironment>
		<HostList>
			<Host Name="PPRO" Version="9.0" />
		</HostList>

	<LocaleList>
		<Locale Code="All" />
	</LocaleList>

	<RequiredRuntimeList>
		<RequiredRuntime Name="CSXS" Version="4.0" />
	</RequiredRuntimeList>
</ExecutionEnvironment>

	<DispatchInfoList>
		<Extension Id="com.lazy.sorter.panel">
			<DispatchInfo>

			<Resources>
				<MainPath>./index.html</MainPath>
				<ScriptPath>./jsx/premiere.jsx</ScriptPath>
			</Resources>

			<Lifecycle>
				<AutoVisible>true</AutoVisible>
			</Lifecycle>

			<UI>

				<Type>Panel</Type>
				<Menu>Lazy Sorter</Menu>

				<Geometry>
					<Size>
						<Height>580</Height>
						<Width>200</Width>
					</Size>
				
					
				</Geometry>

			</UI>
		</DispatchInfo>
	</Extension>
</DispatchInfoList>
</ExtensionManifest>
